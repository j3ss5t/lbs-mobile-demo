{
  "name": "lbs-mobile",
  "version": "0.0.1",
  "description": "Example using preact-mdl with TypeScript and Webpack",
  "author": "Trevor Bekolay",
  "license": "MIT",
  "main": "dist/main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tbekolay/preact-mdl-example.git"
  },
  "bugs": {
    "url": "https://github.com/tbekolay/preact-mdl-example"
  },
  "private": true,
  "scripts": {
    "build": "webpack --debug --devtool source-map --output-pathinfo",
    "debug": "webpack-dev-server --inline --debug --devtool source-map --output-pathinfo",
    "release": "webpack --optimize-minimize --optimize-occurence-order",
    "style:css": "stylelint src/**/*.css",
    "style:ts": "tslint src/**/*.ts*",
    "style": "npm run style:ts && npm run style:css"
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "dependencies": {
    "autoprefixer": "^6.5.3",
    "css-loader": "^0.26.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "material-design-icons": "^3.0.1",
    "material-design-lite": "^1.2.1",
    "postcss-loader": "^1.1.1",
    "postcss-nested": "^1.0.0",
    "preact": "^8.2.0",
    "preact-compat": "^3.16.0",
    "preact-mdl": "^1.3.1",
    "preact-router": "^2.3.2",
    "react-notifications": "^1.4.3",
    "react-websocket": "^1.1.7",
    "style-loader": "^0.13.1",
    "stylelint": "^7.4.2",
    "stylelint-config-standard": "^15.0.0",
    "ts-loader": "^1.3.2",
    "tslint": "^4.0.2",
    "typescript": "^2.0.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2",
    "webpack-fail-plugin": "^1.0.5"
  },
  "moduleNameMapper": {
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
    "\\.(css|less)$": "identity-obj-proxy",
    "^react$": "preact-compat",
    "^react-dom$": "preact-compat"
  },
  "homepage": "https://github.com/tbekolay/preact-mdl-example#readme",
  "devDependencies": {}
}
